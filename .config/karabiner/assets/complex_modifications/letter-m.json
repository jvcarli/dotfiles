{
  "title": "Change M key",
  "rules": [
    {
      "description": "Map Hyper Key + M to return",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "modifiers": {
              "mandatory": [
                "left_control",
		"left_option",
                "left_command",
		"left_shift"
              ],
              "optional": [
                "caps_lock"
              ]
            },
            "key_code": "m"
          },
          "to": [
            {
              "repeat": true,
              "key_code": "return_or_enter"
            }
          ]
        }
      ]
    }
  ]
}
