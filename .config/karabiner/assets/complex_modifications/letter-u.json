{
  "title": "Change U key",
  "rules": [
    {
      "description": "Map Hyper + U to acute accent",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "modifiers": {
              "mandatory": [
                "left_control",
		            "left_option",
	            	"left_command",
	            	"left_shift"
              ],
              "optional": [
                "caps_lock"
              ]
            },
            "key_code": "u"
          },
          "to": [
            {
              "repeat": true,
              "key_code": "e",
              "modifiers": [
                "right_option"
              ]
            }
          ]
        }
      ]
    }
  ]
}
